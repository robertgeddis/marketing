select distinct 
  pv.countrycode,
  pv.visitorId, 
  pv.sessionid,
  pv.pageUrl as landing_page,
  case when lower(pageUrl) like '%sem%' then 'SEM' 
       when lower(pageUrl) like '%online%' then 'Online' 
       when lower(pageUrl) like '%affiliate%' then 'Affiliate' 
    else null end as campaign, 
  case when lower(pv.pageUrl) like '%qualityclick%' then 'QualityClick' else split_part(pv.pageUrl,'|',2) end as site,
  split_part(pv.pageUrl,'|',3) as audience,
  split_part(pv.pageUrl,'|',4) as service,
  split_part(pv.pageUrl,'|',5) as creativeVersion,
  split_part(pv.pageUrl,'|',6) as keyword,
  split_part(pv.pageUrl,'|',7) as campaignName, 
  split_part(pv.pageUrl,'|',8) as adGroupName, 
  vt.memberid as memberid   

from intl.hive_event pv 
	join intl.hive_visit vt on pv.visitorId = vt.visitorId and pv.sessionId = vt.sessionId and date(pv.dateCreated) = date(vt.startDate)

where pv.name = 'PageView'
  and pv.year = 2024
  and pv.month = 7
  and date(pv.datecreated) between '2024-07-08' and '2024-07-19'
  and (pv.currentPageUrl like '%join-now%' or pv.currentpageurl like '%register%') 
  and pv.pageUrl like '%/lp/%'
  and lower(pv.campaign) = 'direct'
